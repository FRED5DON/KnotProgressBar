        <attr name="kpb_progress_cell_height" format="float|dimension" />
        <attr name="kpb_progress_cell_width" format="float|dimension" />
        <attr name="kpb_knot_count" format="integer" />
        <attr name="kpb_knot_radius" format="float|dimension" />
        <attr name="kpb_progressbar_color" format="color" />
