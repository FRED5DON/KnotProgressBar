        <attr name="kpb_background_progressbar_color" format="color" />
        <attr name="kpb_background_label_font_color" format="color" />
        <attr name="kpb_current_pos" format="integer" />
        <attr name="kpb_labels" format="string" />
        <attr name="kpb_content_margin" format="float|dimension" />
        <attr name="kpb_vertical_spacing" format="float|dimension" />
        <attr name="kpb_horizontal_margin" format="float|dimension" />
        <attr name="kpb_label_font_size" format="dimension" />
